{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.0.9",
    "ext-ctype": "*",
    "ext-ds": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openswoole": "*",
    "psr/log": "^3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "openswoole/ide-helper": "@dev"
  }
}
